{
  "name": "map-share-service",
  "version": "1.0.0",
  "description": "Small app to generate links to maps generated by azgaar/fantasy-map-generator.",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "micro build/index.js",
    "build": "tsc --build",
    "d:tsc": "yarn tsc --watch",
    "d:micro": "yarn micro-dev build/index.js --watch build",
    "dev": "concurrently --restart-tries 50 -c cyan,green npm:d:tsc npm:d:micro",
    "makeProductionBuild": "node ./scripts/productionBuild",
    "deploy": "yarn makeProductionBuild && node ./scripts/deploy"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/micro": "7.3.3",
    "@types/micro-cors": "0.1.0",
    "@types/node": "10.12.8",
    "concurrently": "4.1.0",
    "forever": "0.15.3",
    "micro-dev": "3.0.0",
    "sass": "1.15.1",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "@types/express-fileupload": "1.1.0",
    "dropbox": "4.0.13",
    "isomorphic-fetch": "2.2.1",
    "micro": "9.3.3",
    "micro-cors": "0.1.1",
    "micro-upload": "1.0.1"
  }
}
